- if messages
  %h3{class: 'text-center bg-danger'} #{messages}
- else
  %h3{class: 'text-center bg-success'} Created Mock Data
  %dl{class: 'dl-horizontal'}
    %dt Mock Name
    %dd #{mock_name}
    %dt Mock URL
    %dd #{mock_request_url}
    %dt Mock Environment
    %dd #{mock_environment}
    %dt Mock State
    %dd #{mock_state}